{
    "rewrites": [
        { "source": "/(.*)", "destination": "/src/api" },
        { "source": "/auth/(.*)", "destination": "/src/api/authServer.js" },
        { "source": "/chat/(.*)", "destination": "/src/api/chatServer.js" },
        { "source": "/upload/(.*)", "destination": "/src/api/uploadServer.js" }
    ]
}
